@model Team

<div class="col-lg-3 col-md-6 d-flex align-items-stretch mt-5 m-auto " data-aos="fade-up" data-aos-delay="100">
    <div class="member">
        <div class="member-img">
            <img src="@Model.ImgSrc" class="img-fluid" alt="">
            <div class="social">
                <a href=""><i class="bi bi-twitter"></i></a>
                <a href=""><i class="bi bi-facebook"></i></a>
                <a href=""><i class="bi bi-instagram"></i></a>
                <a href=""><i class="bi bi-linkedin"></i></a>
            </div>
        </div>
        <div class="member-info">
            <h4><a href="">@Model.Name</a></h4>
            <span>@Model.Position</span>
        </div>
    </div>
</div>
